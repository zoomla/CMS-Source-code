@using System.Data
@using ZoomLa.BLL.Helper
@model ZoomLa.SQLDAL.SQL.PageSetting
@{  Model.cols = 7;
    Model.url = "StudentManage_Data";
    DataTable dt = new DataTable();
}
<table class="table table-striped table-bordered table-hover margin_t5">
    <tr>
        <td></td>
        <td>成员名称</td>
        <td>成员角色</td>
        <td>申请时间</td>
        <td>状态</td>
        <td>申请备注</td>
        <td>操作</td>
    </tr>
    @foreach(DataRow dr in dt.Rows)
    {
        <tr>
        <td><input type="checkbox" name="idchk" value="@dr["noteid"]" /></td>
        <td>@dr["UserName"]</td>
        <td><%#GetStuType() %></td>
        <td>@DateHelper.ToDate(dr["AddTime"])</td>
        <td><%#GetStatus() %></td>
        <td>@dr["AuditingContext"]</td>
        <td>操作</td>
    </tr>
    }
    @Html.Partial("_page",Model)
</table>
