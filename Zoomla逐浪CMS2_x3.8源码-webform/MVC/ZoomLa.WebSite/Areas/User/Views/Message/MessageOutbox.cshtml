@model ZoomLa.SQLDAL.SQL.PageSetting
@{
    Layout = "~/Areas/User/Views/Shared/_layout.cshtml";
}
@section head{
<title>发件箱</title>
}
@section content{
<div id="pageflag" data-nav="index" data-ban="pub"></div>
<div class="container margin_t5">
    <ol class="breadcrumb">
        <li><a title="会员中心" href="@Url.Action("Default","")">会员中心</a></li>
        <li><a href="@Url.Action("Message", "Message")">消息中心</a></li>
        <li class="active">发件箱</li>
    </ol>
</div>
<div class="container">
    @Html.Partial("MessageTop")
</div>
<div class="container btn_green margin_t10">
    <div class="input-group" style="width: 350px; margin-bottom: 5px;">
        <input type="text" id="skey_t" class="form-control text_300" placeholder="请输入邮件名称" />
        <span class="input-group-btn">
            <button id="skey_btn" class="btn btn-primary" onclick="Skey_Btn_Click" >搜索</button> 
        </span>
    </div>
    @Html.Partial("MessageOutbox_List",Model)
    <button id="batdel_btn"  onclick="BatDel_Btn_Click" class="btn btn-primary">批量删除</button>
</div>
}
@section script{

}
