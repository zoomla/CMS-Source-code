@using System.Data
@using ZoomLa.BLL.Helper
@using ZoomLa.SQLDAL
@model ZoomLa.SQLDAL.SQL.PageSetting
@{
    Model.cols = 7;
    Model.url = "FiServer_Data";
}

<table class="table table-striped table-bordered table-hover">
    <tr class="title" style="height: 24px; text-align: center">
        <td>编号</td>
        <td>标题</td>
        <td>优先级</td>
        <td>问题类型</td>
        <td>已读次数</td>
        <td>提交时间</td>
        <td>状态</td>
    </tr>
    @foreach (DataRow dr2 in Model.dt.Rows)
    {
        <tr align="center" onmouseover="this.className='tdbgmouseover'" onmouseout="this.className='tdbg'" class="tdbg">
            <td>@dr2["questionid"]</td>
            <td>
                <a href="FiServerInfo?QuestionId=@dr2["questionid"]">@dr2["Title"]</a>
            </td>
            <td>@dr2["Priority"]</td>
            <td><a href='SelectiServer?type=(@dr2["type"])' target="_self">@dr2["type"]</td>
            <td>@dr2["ReadCount"]</td>
            <td>@DateHelper.ToDate(dr2["SubTime"])</td>
            <td><label id="lblState">@dr2["State"]</label></td>
        </tr>
    }
    @Html.Partial("_page",Model)
</table>

