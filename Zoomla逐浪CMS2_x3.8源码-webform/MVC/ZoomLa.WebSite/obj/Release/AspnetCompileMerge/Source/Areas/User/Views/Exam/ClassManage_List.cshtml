@using System.Data
@model ZoomLa.SQLDAL.SQL.PageSetting
@{  
    Model.cols = 13;
    Model.url = "ClassMange_Date";
    DataTable dt = new DataTable();
}
<div>
    <table class="table table-striped table-bordered table-hover" id="EGV">
        <tr>
            <td class="td_s"></td>
            <td>名称</td>
            <td>班标</td>
            <td>所属学校</td>
            <td>班主任</td>
            <td>是否毕业</td>
            <td>班级星级</td>
            <td>建立时间</td>
            <td>学生</td>
            <td>教师</td>
            <td>家长</td>
            <td>审核状态</td>
            <td class="title">操作</td>
        </tr>
        @foreach (DataRow dr in Model.dt.Rows)
        {
            <tr>
                <td><input type='checkbox' id='chkAll' /></td>

                <td>
                    <input name="Item" type="checkbox" value="@dr["RoomID"]" />
                </td>
                <td>
                    @dr["RoomName"]
                </td>
                <td>
                    <%#GetIcon() %>
                </td>
                <td>
                    <a href="SchoolShow?id=<%#@dr["SchoolId"]%>">@dr["SchoolName"]</a>
                </td>
                <td>
                    @dr["UserName"]
                </td>
                <td>
                    <%#GetIsDone() %>
                </td>
                <td>
                    <%#GetStar() %>
                </td>
                <td>
                    @dr["Creation"]
                </td>
                <td>
                    @dr["StuCount"]
                </td>
                <td>
                    @dr["TeachCount"]
                </td>
                <td>
                    @dr["FamilyCount"]
                </td>
                <td>
                    <%#GetStatu() %>
                </td>

                <td class="text-center">
                    <a href="ClassShow?cid=<%#@dr["roomid"]%> " title="浏览"><span class="fa fa-eye"></span></a>
                    <%#GetOP() %>
                </td>
            </tr>
        }
        @Html.Partial("_page",Model)
    </table>
</div>
