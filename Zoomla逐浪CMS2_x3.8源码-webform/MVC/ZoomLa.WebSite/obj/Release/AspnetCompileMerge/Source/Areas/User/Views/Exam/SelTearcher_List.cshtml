@using System.Data
@model ZoomLa.SQLDAL.SQL.PageSetting
@{
    Model.cols = 4;
    Model.url = "SelTearchar_Data";
    DataTable dt = new DataTable();
}
<table class="table table-bordered table-striped table-hover margin_t10">
    <tr>
        <td></td>
        <td>用户组</td>
        <td>用户名</td>
        <td>真实姓名</td>
    </tr>
    @foreach (DataRow dr in dt.Rows)
    {
        <tr>
            <td><input type="checkbox" data-name="@dr["username"]" name="idchk" value="@dr["UserID"]" /></td>
            <td>@dr["GroupName"]</td>
            <td>@dr["UserName"]</td>
            <td>@dr["HoneyName"]</td>
        </tr>
    }
    @Html.Partial("_page", Model)
</table>
