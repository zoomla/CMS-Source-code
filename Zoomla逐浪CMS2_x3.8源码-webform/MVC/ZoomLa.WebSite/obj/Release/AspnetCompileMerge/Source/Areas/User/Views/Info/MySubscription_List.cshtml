@using System.Data
@using ZoomLa.BLL.Helper
@model ZoomLa.SQLDAL.SQL.PageSetting
@{
  Model.cols=5;
  Model.Check=false;
}
<table class="table table-striped table-bordered" id="EGV">
	<tr><td colspan="5" class="text-center">我的订阅管理</td></tr>
	<tr>
		<td>编号</td>
		<td>订阅标题</td>
		<td>生效日期</td>
		<td>到期日期</td>
		<td>操作</td>
	</tr>
@foreach (DataRow dr in Model.dt.Rows)
{
    <tr>
        <td>@dr["Id"]</td>
        <td><a href="javascript::">@dr["title"]</a></td>
        <td class="td_l">@DateHelper.ToDate(dr["effectTime"])</td>
        <td class="td_l">@DateHelper.ToDate(dr["endTime"])</td>
        <td class="td_l">
            <a href="javascript:;">删除</a>
        </td>
    </tr>
}
@Html.Partial("_page",Model)
</table>